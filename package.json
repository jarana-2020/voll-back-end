{
  "name": "voll_back",
  "version": "1.0.0",
  "description": "Teste Tecnico Voll Sollutions",
  "main": "./build/src/api/server",
  "scripts": {
    "start": "npm run build && node ./build/api/server.js",
    "build": "tsc",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "dev": "ts-node ./src/api/server.ts",
    "lint": "eslint ./src --ext .ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/tests/**/*.ts'"
  },
  "author": "Julio Cesar Arana",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "sinon": "^13.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/md5": "^2.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1"
  }
}
